library("rpart.plot")
titanic_data <- read.csv("titanic_data.csv")
glimpse(titanic_data)
str(titanic_data)
titanic_data <- titanic_data %>% mutate(AnzahlFamilienmitglieder = SibSp + Parch)
titanic_data <- titanic_data %>% mutate(Altersklasse = case_when(
Age >= 18 ~ "Erwachsen",
Age < 18 ~ "Kinder"
))
survival_data <- titanic_data %>% group_by(Sex, Age) %>% summarise(Survived = mean(Survived, na.rm = TRUE))
survival_data %>% ggplot(aes(x=Age, y = Survived, colour = Sex)) + geom_point()
price_survive <- titanic_data %>% summarise(.by = Fare, Survived = mean(Survived, na.rm = TRUE))
price_survive %>% ggplot(aes(x=Fare, y = Survived)) + geom_point()
class_survive <- titanic_data %>% summarise(.by = Pclass, Survived = mean(Survived, na.rm = TRUE))
class_survive %>% ggplot(geom_bar(aes(x=Survived)))
