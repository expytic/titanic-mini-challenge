library("ggalluvial")
library("rpart")
library("rpart.plot")
titanic_data <- read.csv("titanic_data.csv")
glimpse(titanic_data)
str(titanic_data)
titanic_data <- titanic_data %>% mutate(AnzahlFamilienmitglieder = SibSp + Parch)
titanic_data <- titanic_data %>% mutate(Altersklasse = case_when(
Age >= 18 ~ "Erwachsen",
Age < 18 ~ "Kinder"
))
male <- titanic_data %>% filter(Sex == "male") %>% summarise(.by = Age, Survived = mean(Survived, na.rm = TRUE), Sex = "male")
female <- titanic_data %>% filter(Sex == "female") %>% summarise(.by = Age, Survived = mean(Survived, na.rm = TRUE), Sex = "female")
survival_data <- rbind(female, male)
male %>% ggplot(aes(x=Age, y = Survived)) + geom_point()
